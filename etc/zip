x = [1, 2, 3]
y = [4, 5, 6]

zipped = zip(x, y)
print("list(zipped)  :   ", list(zipped))
'''
결과
[(1, 4), (2, 5), (3, 6)]
'''

z = [7, 8, 9]
zipped2 = zip(x, y, z)
print("list(zipped2)   :   ", list(zipped2))
'''
결과
[(1, 4, 7), (2, 5, 8), (3, 6, 9)]
'''

## 2) str 형
a = 'abc'
b = 'def'

zipped3 = zip(a, b)
print("list(zipped3)   :   ", list(zipped3))
'''
결과
[('a', 'd'), ('b', 'e'), ('c', 'f')]
'''